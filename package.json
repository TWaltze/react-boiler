{
  "name": "react-boiler",
  "version": "1.0.0",
  "description": "Boilerplate for ReactJS project with hot code reloading",
  "main": "index.js",
  "scripts": {
    "lint": "eslint --ext .js,.jsx ./",
    "flow": "flow --quiet",
    "clean": "rimraf dist",
    "start": "cross-env NODE_ENV=development webpack-serve --config ./webpack/webpack.config.js",
    "build": "cross-env NODE_ENV=production webpack --config webpack/webpack.config.js"
  },
  "author": "Tyler Waltze <hello@tylerwaltze.com> (http://github.com/twaltze)",
  "license": "MIT",
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.3",
    "babel-loader": "^7.1.4",
    "babel-preset-env": "^1.7.0",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "cross-env": "^5.1.6",
    "css-loader": "^0.28.11",
    "empty": "^0.10.1",
    "eslint": "^4.19.1",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-import-resolver-webpack": "^0.10.0",
    "eslint-loader": "^2.0.0",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-react": "^7.9.1",
    "flow-bin": "^0.74.0",
    "html-webpack-plugin": "^3.2.0",
    "npm-run-all": "^4.1.3",
    "rimraf": "^2.6.2",
    "style-loader": "^0.21.0",
    "webpack": "^4.12.0",
    "webpack-merge": "^4.1.2",
    "webpack-serve": "^1.0.4"
  },
  "dependencies": {
    "@fortawesome/fontawesome": "^1.1.8",
    "@fortawesome/fontawesome-free-solid": "^5.0.13",
    "@fortawesome/react-fontawesome": "^0.0.20",
    "lodash": "^4.17.10",
    "react": "^16.4.0",
    "react-dom": "^16.4.0",
    "react-hot-loader": "^4.3.1",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.3.1",
    "redux": "^4.0.0"
  }
}
